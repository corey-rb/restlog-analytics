
  <div class="header-main">

    <div class="app-name">
      <span>{{app.name}}</span>
    </div>

    <div class="user-drop">
      <span>{{ username }}</span>
    </div>

    <div class="notifications-drop">
      <span>3</span>
    </div>

  </div>

  <div class="header-sub">

    <div class="drop-notifications">
      <div class="number">You have 3 notifications:</div>
      <div class="notifications-list">
        <div>
          Notification #1
        </div>
        <div>
          Notification #2
        </div>
        <div>
          Notification #3
        </div>
      </div>
    </div>

    <div class="drop-user">
      <div class="notifications-list">
        <div>
          Data Usage: 200mb of 500mb
        </div>
        <div>
          <a ui-sref="app.docs">New</a>
        </div>
        <div>
          <a href="#">Profile</a>
        </div>
        <div>
          <a ui-sref="access.signin">Logout</a>
        </div>
      </div>
    </div>

  </div>






